.logo{
  display: block;
  height: 250rpx;
  width: 250rpx;
  border-radius: 125rpx;
  margin: 0 auto;
  margin-top: 100rpx;
}

.title{
  width: 100%;
  height: 90rpx;
  /* 字体居中 */
  text-align: center;
  /* 垂直居中 */
  line-height: 90rpx;
  /* 设置字体大小 */
  font-size: 30rpx;
  /* 设置字体颜色 */
  color: #ccc;
}

.btn{
  width: 60%;
  border: 1rpx solid burlywood;
  /* 设置字体 */
  color: rgb(199, 128, 36);
  margin-top: 400rpx;
}